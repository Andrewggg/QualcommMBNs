{
  "DeviceConfigXml": {
    "Values": [
      "?xml version=\"1.0\" encoding=\"UTF-8\"?>\r",
      "\r",
      "<!-- 7+5 mode Device configuration file for CMCC subsidized devices\r",
      "$Header: //commercial/MPSS.TA.2.3.c1/Main/modem_proc/mmcp/policyman/configurations/DevConfig/7+5_mode/1x+L/device_config.xml#3 $\r",
      "-->\r",
      "\r",
      "<policy name        = \"generic\"\r",
      "        changelist  = \"$Change: 15535930 $\"\r",
      "        schema_ver  = \"1\"\r",
      "        policy_ver  = \"2029.1.4\"\r",
      ">\r",
      "\r",
      "  <initial>\r",
      "    <actions>\r",
      "\r",
      "      <!-- Define device configuration as DSDS -->\r",
      "      <device_configuration num_sims=\"2\" max_active_voice=\"1\" max_active_data=\"1\">\r",
      "        <config primary=\"C H G W L T\" secondary=\"G\" />\r",
      "        <config primary=\"C H G\" secondary=\"G W L T\" />\r",
      "        <e911_config primary=\"C H G W L T\" secondary=\"G\" />\r",
      "        <e911_config primary=\"C H G\" secondary=\"G W L T\" />\r",
      "      </device_configuration>\r",
      "\r",
      "      <!-- These are the CMCC IMSI PLMNs -->\r",
      "      <plmn_list name= \"cmcc_imsi\" ns=\"global\">\r",
      "        460-00 460-02 460-07 460-08\r",
      "      </plmn_list>\r",
      "\r",
      "      <!-- These are the CT IMSI MCCs -->\r",
      "      <mcc_list name=\"ct_mccs\" ns=\"global\">\r",
      "        455 460\r",
      "      </mcc_list>\r",
      "\r",
      "      <boolean_define name=\"cm:reject_dds_change\" ns=\"global\" initial=\"false\" />\r",
      "      <boolean_define name=\"cm:reject_1xdata\" ns=\"global\" initial=\"true\" />\r",
      "      <boolean_define name=\"cm:start_ps_sub\" ns=\"global\" initial=\"false\" />\r",
      "\r",
      "    </actions>\r",
      "  </initial>\r",
      "\r",
      "  <!-- RULE #1 -->\r",
      "  <!-- DDS change is not allowed only for CT+CMCC(DDS) -> CT(DDS)+CMCC.\r",
      "  -->\r",
      "  <if precond=\"none\">\r",
      "    <cond> <true /> </cond>\r",
      "    <then>\r",
      "      <boolean_set name=\"cm:reject_dds_change\">\r",
      "        <any_of>\r",
      "\r",
      "          <!-- CMCC(DDS)+CT -->\r",
      "          <all_of>\r",
      "            <is_subs_dds subs=\"this\" />\r",
      "            <imsi_plmn_in subs=\"this\" list=\"cmcc_imsi\" />\r",
      "              <imsi_mcc_in subs=\"other\" session=\"1x\" list=\"ct_mccs\" />\r",
      "          </all_of>\r",
      "\r",
      "          <!-- CT+CMCC(DDS) -->\r",
      "          <all_of>\r",
      "            <is_subs_dds subs=\"other\" />\r",
      "            <imsi_plmn_in subs=\"other\" list=\"cmcc_imsi\" />\r",
      "            <imsi_mcc_in subs=\"this\" session=\"1x\" list=\"ct_mccs\" />\r",
      "          </all_of>\r",
      "          \r",
      "        </any_of>\r",
      "      </boolean_set>\r",
      "      <continue/>\r",
      "    </then>\r",
      "  </if>\r",
      "\r",
      "  <!-- RULE #1 -->\r",
      "  <!-- Start PS SUB first if we have a CMCC card -->\r",
      "  -->\r",
      "  <if precond=\"none\">\r",
      "    <cond> <true /> </cond>\r",
      "    <then>\r",
      "      <boolean_set name=\"cm:start_ps_sub\">\r",
      "        <any_of>\r",
      "          <imsi_plmn_in subs=\"this\" list=\"cmcc_imsi\" />\r",
      "          <imsi_plmn_in subs=\"other\" list=\"cmcc_imsi\" />\r",
      "        </any_of>\r",
      "      </boolean_set>\r",
      "      <continue/>\r",
      "    </then>\r",
      "  </if>\r",
      "\r",
      "\r",
      "  <!-- RULE #2 -->\r",
      "  <!-- If two CMCC cards, the one with DDS gets most capability -->\r",
      "  <if precond=\"none\">\r",
      "    <cond>\r",
      "      <imsi_plmn_in subs=\"this\" list=\"cmcc_imsi\" />\r",
      "      <imsi_plmn_in subs=\"other\" list=\"cmcc_imsi\" />\r",
      "    </cond>\r",
      "    <then>\r",
      "      <if>\r",
      "       <cond> <is_subs_dds /> </cond>\r",
      "       <then> <select_config index=\"0\" /> </then>\r",
      "       <else> <select_config index=\"1\" /> </else>\r",
      "      </if>\r",
      "    </then>\r",
      "  </if>\r",
      "\r",
      "  <!-- If only one CMCC card it gets max capability -->\r",
      "  <!-- RULE #3 -->\r",
      "  <if precond=\"none\">\r",
      "    <cond> <imsi_plmn_in subs=\"this\" list=\"cmcc_imsi\" /> </cond>\r",
      "    <then> <select_config index=\"0\" /> </then>\r",
      "  </if>\r",
      "\r",
      "  <!-- RULE #4 -->\r",
      "  <if precond=\"none\">\r",
      "    <cond> <imsi_plmn_in subs=\"other\" list=\"cmcc_imsi\" /> </cond>\r",
      "    <then> <select_config index=\"1\" /> </then>\r",
      "  </if>\r",
      "\r",
      "  <!-- If two foreign cards, the one with DDS gets most capability -->\r",
      "  <!-- RULE #5 -->\r",
      "  <if precond=\"none\">\r",
      "    <cond>\r",
      "      <not> <imsi_mcc_in subs=\"this\" list=\"home_mccs\" /> </not>\r",
      "      <not> <imsi_mcc_in subs=\"other\" list=\"home_mccs\" /> </not>\r",
      "    </cond>\r",
      "    <then>\r",
      "      <if>\r",
      "       <cond> <is_subs_dds /> </cond>\r",
      "       <then> <select_config index=\"0\" /> </then>\r",
      "       <else> <select_config index=\"1\" /> </else>\r",
      "      </if>\r",
      "    </then>\r",
      "  </if>\r",
      "\r",
      "  <!-- If only one foreign card, it gets max capability. Otherwise, max cap follows DDS -->\r",
      "  <!-- RULE #6 -->\r",
      "  <if precond=\"none\">\r",
      "    <cond> <not> <imsi_mcc_in subs=\"this\" list=\"home_mccs\" /> </not> </cond>\r",
      "    <then> <select_config index=\"0\" /> </then>\r",
      "  </if>\r",
      "\r",
      "  <!-- RULE #7 -->\r",
      "  <if precond=\"none\">\r",
      "    <cond> <not> <imsi_mcc_in subs=\"other\" list=\"home_mccs\" /> </not> </cond>\r",
      "    <then> <select_config index=\"1\" /> </then>\r",
      "  </if>\r",
      "\r",
      "  <!-- Otherwise follow normal DDS rules -->\r",
      "  <!-- RULE #8 -->\r",
      "  <if precond=\"none\">\r",
      "    <cond> <true /> </cond>\r",
      "    <then> <select_config index=\"-1\" /> </then>\r",
      "  </if>\r",
      "\r",
      "</policy>\r"
    ]
  },
  "CarrierPolicyXml": {
    "Values": [
      "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\r",
      "\r",
      "<!-- Carrier Policy for CMCC CSFB configuration with GWL roaming\r",
      "$Header: //commercial/MPSS.TA.2.3.c1/Main/modem_proc/mmcp/policyman/configurations/Carrier/CMCC/subsidized/carrier_policy.xml#6 $\r",
      "-->\r",
      "<policy name        = \"generic\"\r",
      "        changelist  = \"$Change: 15620961 $\"\r",
      "        schema_ver  = \"1\"\r",
      "        policy_ver  = \"76.1.31\"\r",
      ">\r",
      "\r",
      "  <initial>\r",
      "    <actions>\r",
      "\r",
      "      <!-- Define the Home MCCs -->\r",
      "      <mcc_list name=\"home_mccs\"> 460 </mcc_list>\r",
      "\r",
      "      <mcc_list name=\"lw_roam_mccs\"> 440 441 450 </mcc_list>\r",
      "\r",
      "      <!-- These are the CMCC IMSI PLMNs -->\r",
      "      <plmn_list name = \"unrestricted_operators\">\r",
      "        460-00 460-02 460-04 460-07 460-08\r",
      "      </plmn_list>\r",
      "\r",
      "      <plmn_list name = \"ca_combo_operators\">\r",
      "        460-00 460-02 460-04 460-07 460-08\r",
      "      </plmn_list>\r",
      "\r",
      "      <!-- These are the CU IMSI PLMNs -->\r",
      "      <plmn_list name=\"cu_imsi\">\r",
      "        460-01 460-06 460-09\r",
      "      </plmn_list>\r",
      "\r",
      "      <!-- These are the CT IMSI MCCs -->\r",
      "      <mcc_list name=\"ct_mccs\">\r",
      "        455 460\r",
      "      </mcc_list>\r",
      "\r",
      "      <boolean_define name=\"change_rat_order_in_full_rat\" initial=\"true\" />\r",
      "\r",
      "      <define_fullrat_config>\r",
      "        <actions>   \r",
      "          <rf_bands base=\"hardware\" />\r",
      "          <if>\r",
      "            <cond> <boolean_test name=\"change_rat_order_in_full_rat\" /> </cond>\r",
      "            <then> <rat_order> G W L T C H </rat_order> </then>\r",
      "          </if>\r",
      "          <if>\r",
      "           <cond>\r",
      "             <imsi_mcc_in session=\"1x\" list=\"ct_mccs\" />\r",
      "           </cond>\r",
      "           <then>\r",
      "             <rat_capability base=\"none\">\r",
      "               <include> CDMA HDR GSM </include>\r",
      "             </rat_capability>\r",
      "             <svc_mode> ALL </svc_mode>\r",
      "             <scan_optimization tgpp=\"no_auto_camp_only\" tgpp2=\"wrlf\" />\r",
      "           </then>\r",
      "           <else>\r",
      "             <rat_capability base=\"none\">\r",
      "               <include> TDSCDMA LTE GSM WCDMA </include>\r",
      "             </rat_capability>\r",
      "             <svc_mode> LIMITED </svc_mode>\r",
      "             <scan_optimization tgpp=\"disabled\" tgpp2=\"disabled\" />\r",
      "           </else>\r",
      "          </if>\r",
      "       </actions>\r",
      "      </define_fullrat_config>\r",
      "\r",
      "      <!-- Home RF Band list --> \r",
      "      <rf_band_list name=\"rf_bands_home\">\r",
      "        <gw_bands base=\"hardware\" />\r",
      "        <!--  Refer to sys_sband_lte_e_type in sys.h for band mapping -->\r",
      "        <lte_bands base=\"none\" >\r",
      "          <include> 2 7 33 37 38 39 40 </include>\r",
      "        </lte_bands> \r",
      "        <tds_bands base=\"hardware\" />\r",
      "      </rf_band_list>\r",
      "\r",
      "      <!-- Set initial power up capability -->\r",
      "      <rat_capability_if base=\"none\">\r",
      "        <include> GSM TDSCDMA LTE </include>\r",
      "      </rat_capability_if>\r",
      "      <rf_bands_if list=\"rf_bands_home\" />\r",
      "\r",
      "      <ue_mode> CSFB </ue_mode>\r",
      "\r",
      "      <feature single_sim=\"normal\"> dualmmode </feature>\r",
      "\r",
      "      <boolean_define name=\"home\" initial=\"true\" />\r",
      "      <ca_band_combos string=\"\" />\r",
      "\r",
      "    </actions>\r",
      "  </initial>\r",
      "\r",
      "  <!-- RULE #1 -->\r",
      "  <if precond=\"none\">\r",
      "    <not> <phone_operating_mode> ONLINE </phone_operating_mode> </not>\r",
      "  <then>\r",
      "    <stop />\r",
      "  </then>\r",
      "  </if>\r",
      "\r",
      "  <!-- RULE #2 -->\r",
      "  <if precond=\"none\">\r",
      "    <volte_enabled />\r",
      "    <then>\r",
      "      <voice_domain_pref mandatory=\"false\"> IMS_PREF </voice_domain_pref>\r",
      "      <continue />\r",
      "    </then>\r",
      "    <else>\r",
      "      <voice_domain_pref mandatory=\"true\"> CS_ONLY </voice_domain_pref>\r",
      "      <continue />\r",
      "    </else>\r",
      "  </if>\r",
      "\r",
      "<!-- RULE #3 -->\r",
      "  <!-- Restrict CU cards to GSM only.\r",
      "  -->\r",
      "  <rule precond=\"IMSI\">\r",
      "    <conditions>\r",
      "      <imsi_plmn_in list=\"cu_imsi\" />\r",
      "    </conditions>\r",
      "    <actions>\r",
      "      <rat_capability base=\"none\">\r",
      "        <include> GSM </include>\r",
      "      </rat_capability>\r",
      "      <rf_bands />\r",
      "      <svc_mode> FULL </svc_mode>\r",
      "      <rat_order> G W L T C H </rat_order>\r",
      "      <scan_optimization tgpp=\"disabled\" tgpp2=\"disabled\" />\r",
      "    </actions>\r",
      "  </rule>\r",
      "\r",
      "  <!-- RULE #4 -->\r",
      "  <!-- We Wait if we don't have location or we are not ONLINE.\r",
      "  -->\r",
      "  <rule precond=\"none\">\r",
      "    <conditions>\r",
      "      <not> <have_location /> </not>\r",
      "    </conditions>\r",
      "    <actions />\r",
      "  </rule>\r",
      "\r",
      "  <!-- RULE #5 -->\r",
      "  <rule precond=\"SS\">\r",
      "    <conditions>\r",
      "      <true />\r",
      "    </conditions>\r",
      "    <actions>\r",
      "      <if>\r",
      "        <cond> <serving_plmn_in list = \"ca_combo_operators\" /> </cond>\r",
      "        <then> <ca_band_combos string=\"38CA-0;38AA-38A-0;41AA-41A-0;41AA-41A-1;41CA-0;41CA-1;41AA-39A-0;41A-39AA-0;40CA-0;40CA-1;40AA-40A-0;39CA-0;38CC-0;38AA-38AA-0;41AA-41AA-0;41AA-41AA-1;41CC-0;41CC-1;41AA-39AA-0;40CC-0;40CC-1;40AA-40AA-0;39CC-0;41CC-39A-0;41CA-39AA-0;41AA-39CA-0;41A-39CC-0;41AA-41AA-39A-0;41AA-41A-39AA-0;41AA-39AA-39A-0;41A-39AA-39AA-0;41DC-0;\" /> </then>\r",
      "        <else>\r",
      "         <ca_band_combos string=\"\" />\r",
      "        </else>\r",
      "      </if>\r",
      "    <continue />\r",
      "  </actions>\r",
      "  </rule>\r",
      "\r",
      "  <!-- RULE #6 -->\r",
      "  <!-- If we have location, determine UE location and continue evaluation.\r",
      "  -->\r",
      "  <rule precond=\"none\">\r",
      "    <conditions>\r",
      "      <have_location />\r",
      "    </conditions>\r",
      "    <actions>\r",
      "      <svc_mode> FULL </svc_mode>\r",
      "      <boolean_set name=\"home\">\r",
      "        <location_mcc_in list=\"home_mccs\" />\r",
      "      </boolean_set>\r",
      "      <scan_optimization tgpp=\"disabled\" tgpp2=\"disabled\" />\r",
      "      <rat_order> L W T G C H </rat_order>\r",
      "      <boolean_set name=\"change_rat_order_in_full_rat\" value=\"false\" />\r",
      "      <continue />\r",
      "    </actions>\r",
      "  </rule>\r",
      "\r",
      "  <!--\r",
      "  ====================================================================\r",
      "    Rules where location info can be used.\r",
      "  ====================================================================\r",
      "  -->\r",
      "\r",
      "  <!-- RULE #7 -->\r",
      "  <!-- 1x cards in Home get CDMA + HDR and GSM in roaming -->\r",
      "  <rule precond=\"IMSI\">\r",
      "    <conditions>\r",
      "      <imsi_mcc_in session=\"1x\" list=\"ct_mccs\" />\r",
      "    </conditions>\r",
      "    <actions>\r",
      "      <rf_bands />\r",
      "      <if>\r",
      "        <cond> <boolean_test name=\"home\" /> </cond>\r",
      "        <then>\r",
      "          <rat_capability base=\"none\">\r",
      "            <include> CDMA HDR </include>\r",
      "          </rat_capability>\r",
      "        </then>\r",
      "        <else>\r",
      "          <rat_capability base=\"none\">\r",
      "            <include> GSM </include>\r",
      "          </rat_capability>\r",
      "        </else>\r",
      "      </if>\r",
      "    </actions>\r",
      "  </rule>\r",
      "\r",
      "  <!-- RULE #8 -->\r",
      "  <!-- Other cards get TGL in Home -->\r",
      "  <rule precond=\"none\">\r",
      "    <conditions>\r",
      "      <boolean_test name=\"home\" />\r",
      "    </conditions>\r",
      "    <actions>\r",
      "      <rat_capability base = \"none\">\r",
      "        <include> TDSCDMA GSM LTE </include>\r",
      "      </rat_capability>\r",
      "      <rf_bands list=\"rf_bands_home\" />\r",
      "    </actions>\r",
      "  </rule>\r",
      "\r",
      "  <!-- RULE #9 -->\r",
      "  <!-- Other cards in Japan/Korea restrict to LW capability -->\r",
      "  <rule precond=\"none\">\r",
      "    <conditions>\r",
      "      <not> <boolean_test name=\"home\" /> </not>\r",
      "      <location_mcc_in list=\"lw_roam_mccs\" />\r",
      "    </conditions>\r",
      "    <actions>\r",
      "      <rf_bands />\r",
      "      <rat_capability base = \"none\">\r",
      "        <include> LTE WCDMA </include>\r",
      "      </rat_capability>\r",
      "    </actions>\r",
      "  </rule>\r",
      "  \r",
      "  <!-- RULE #10 -->\r",
      "  <!-- Other cards restrict to GWL -->\r",
      "  <rule precond=\"none\">\r",
      "    <conditions>\r",
      "      <true/>\r",
      "    </conditions>\r",
      "    <actions>\r",
      "      <rf_bands />\r",
      "      <rat_capability base=\"none\">\r",
      "        <include> GSM WCDMA LTE </include>\r",
      "      </rat_capability>\r",
      "    </actions>\r",
      "  </rule>\r",
      "\r",
      "</policy>\r"
    ]
  },
